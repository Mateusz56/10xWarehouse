---
import MainLayout from '@/layouts/MainLayout.astro';
import WarehouseDetailsView from '@/components/WarehouseDetailsView.vue';

// Enable server-side rendering for this dynamic route
export const prerender = false;

export interface Props {
  id: string;
}

const { id } = Astro.params as Props;
---

<MainLayout title="Warehouse Details">
  <WarehouseDetailsView client:load warehouseId={id} />
</MainLayout>
